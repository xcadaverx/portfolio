    "eslint": "^3.3.0",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.13.0",
    "mocha": "^3.0.2",
    "nyc": "^8.1.0",
    "standard-version": "^2.4.0"
  },
  "scripts": {
    "lint": "eslint src test",
    "compile": "cross-env NODE_ENV=production babel src --out-dir lib",
